    },
    "Homepage_banner_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name"
    },
    "Portfolio_page_listing_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Mobile_banner_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
